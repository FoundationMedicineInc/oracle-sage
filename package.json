{
  "name": "oracle-sage",
  "description": "Promise driven object modeling for OracleDB",
  "version": "1.1.0",
  "main": "./build/sage.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/mocha/bin/mocha --recursive --timeout 30000",
    "prepublishOnly": "nsp check && babel ./src --out-dir ./build",
    "watch": "babel src -d build -w",
    "build": "babel src -d build",
    "lint": "./node_modules/.bin/eslint --fix \"src/**/*.js\"",
    "prettier": "./node_modules/.bin/prettier --single-quote --trailing-comma --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "nsp check && npm run prettier && npm run lint && npm run build"
    }
  },
  "license": "MIT",
  "author": {
    "name": "Justin Ho",
    "email": "mrchess8@gmail.com",
    "url": "http://github.com/himrc"
  },
  "peerDependencies": {
    "oracledb": "^1.13.1 || ^2.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "async": "^2.1.2",
    "bluebird": "^3.4.6",
    "knex": "0.12.6 ",
    "lodash": "^4.15.0",
    "moment": "^2.15.1",
    "object-assign": "^4.1.0",
    "promise": "^7.1.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "mocha": "^2.3.4",
    "nsp": "^3.2.1",
    "oracledb": "^2.1.2",
    "prettier": "^1.11.1"
  },
  "bugs": {
    "url": "https://github.com/FoundationMedicineInc/oracle-sage/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FoundationMedicineInc/oracle-sage.git"
  }
}
